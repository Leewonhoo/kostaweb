<script type="text/javascript">
	function idchecked(){
		var url = "idchecked.html";
		var winName = "_blank";
		var winconfig = "top=100, left=100, width=400, height=300, resizable=no";
		var childWin = window.open(url, winName, winconfig);
		
		var id = document.getElementById("memberId");
		var child = childWin.document.idcheckk.reid.value;
		id.value = child;
		/* 자식 창의 아이디로 초기값 설정 */
	}
</script>

</head>
<body>
	<form action="join.html" name="idcheckk">
		<table border="1" width="100%" height="100%">
			<tr>
				<td>아이디</td>
				<td colspan="2" width="100px"><input id="reid" name="reid" type="text"></td>
				<td><input type="button" value="재확인"></td>
			</tr>
			<tr>
				<td colspan="4">
					<input type="button" value="사용" onclick="idchecked()">
					<input type="button" value="닫기">
				</td>
			</tr>
